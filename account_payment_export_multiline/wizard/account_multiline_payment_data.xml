<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Charge Codes -->
        <record model="multiline.payment.charge.code" id="multiline_payment_charge_code_SHA">
            <field name="code">SHA</field>
            <field name="description">Shared charges, i.e. :
- charges in Belgium to be borne by the ordering customer
- charges abroad to be borne by the beneficiary customer
            </field>
        </record>

        <record model="multiline.payment.charge.code" id="multiline_payment_charge_code_BEN">
            <field name="code">BEN</field>
            <field name="description">All charges to be borne by the beneficiary customer</field>
        </record>

        <record model="multiline.payment.charge.code" id="multiline_payment_charge_code_OUR">
            <field name="code">OUR</field>
            <field name="description">All charges to be borne by the ordering customer</field>
        </record>

        <!-- Instruction Codes -->
        <record model="multiline.payment.instruction.code" id="multiline_payment_instruction_code_INTC">
            <field name="name">Intergroup payment</field>
            <field name="code">INTC</field>
        </record>

        <record model="multiline.payment.instruction.code" id="multiline_payment_instruction_code_CHQB">
            <field name="name">Check payment</field>
            <field name="code">CHQB</field>
        </record>

        <record model="multiline.payment.instruction.code" id="multiline_payment_instruction_code_OTHER_ADEB">
            <field name="name">Notice of debit</field>
            <field name="code">OTHR/ADEB</field>
        </record>

        <record model="multiline.payment.instruction.code" id="multiline_payment_instruction_code_OTHER_SALA">
            <field name="name">Wage payment</field>
            <field name="code">OTHR/SALA</field>
        </record>

    </data>
</openerp>
